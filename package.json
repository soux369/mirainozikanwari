{
  "name": "university-timetable-mobile",
  "version": "3.1.23",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "node scripts/bump-version.js && node node_modules/eas-cli/bin/run build --platform android",
    "build:android:offline": "node scripts/bump-version.js && cd android && gradlew bundleRelease",
    "build:ios": "node scripts/bump-version.js && node node_modules/eas-cli/bin/run build --platform ios",
    "generate-ads": "node scripts/generate-app-ads.js",
    "eas-build-pre-install": "node scripts/create-secrets.js",
    "postinstall": "node scripts/fix-widget-center.js && node scripts/patch-podfile-plugin.js"
  },
  "dependencies": {
    "@bittingz/expo-widgets": "^3.0.2",
    "@expo-google-fonts/m-plus-rounded-1c": "^0.4.2",
    "@expo-google-fonts/noto-sans-jp": "^0.4.2",
    "@expo-google-fonts/noto-serif-jp": "^0.4.2",
    "@expo-google-fonts/roboto": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@types/react": "~19.1.10",
    "dotenv": "^17.2.3",
    "expo": "~54.0.30",
    "expo-blur": "~15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-camera": "^17.0.10",
    "expo-device": "~8.0.10",
    "expo-document-picker": "^14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-location": "~19.0.8",
    "expo-network": "~8.0.8",
    "expo-notifications": "~0.32.15",
    "expo-sharing": "^14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-tracking-transparency": "~6.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-android-widget": "^0.17.2",
    "react-native-google-mobile-ads": "^16.0.1",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-shared-group-preferences": "^1.1.24",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-widget-center": "^0.0.9",
    "tesseract.js": "^7.0.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "devDependencies": {
    "@react-native-community/cli": "^20.0.2",
    "@types/fs-extra": "^11.0.4",
    "eas-cli": "^16.28.0",
    "fs-extra": "^11.3.3",
    "patch-package": "^8.0.1"
  }
}
